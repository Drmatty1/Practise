#include<stdio.h>
int main(){
    int i,j,k,n;
    printf("Enter Size of Square : ");
    scanf("%d",&n); 
    for(i=1;i<=n;i++){
        k=1;
        for(j=1;j<=n;j++){
            if(i==1||i==n||j==1||j==n||((j<=n-2&&j>=3)&&(i==3||i==n-2))||((i<=n-2&&i>=3)&&(j==3||j==n-2))){
                printf("%d ",k);

            }
            else {
                printf("  ");
            }
            k++;
        }
        printf("\n");
    }

}
